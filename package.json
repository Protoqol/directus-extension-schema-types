{
  "name"              : "@protoqol/directus-extension-schema-types",
  "description"       : "Directus extension to generate Typescript (and more) schemas from a collection.",
  "license"           : "MIT",
  "icon"              : "extension",
  "version"           : "1.1.5",
  "keywords"          : [
    "directus",
    "directus-extension",
    "directus-extension-module"
  ],
  "type"              : "module",
  "files"             : [
    "dist"
  ],
  "directus:extension": {
    "type"  : "module",
    "path"  : "dist/index.js",
    "source": "src/index.ts",
    "host"  : ">=10.10.0"
  },
  "scripts"           : {
    "build"     : "directus-extension build",
    "dev"       : "directus-extension build -w --no-minify",
    "link"      : "directus-extension link",
    "validate"  : "directus-extension validate",
    "type-check": "vue-tsc --noEmit"
  },
  "devDependencies"   : {
    "@directus/extensions-sdk": "17.0.5",
    "typescript"              : "^5.9.3",
    "vue"                     : "^3.5.27",
    "vue-tsc"                 : "^3.2.4"
  },
  "dependencies"      : {
    "@highlightjs/vue-plugin": "^2.1.0"
  }
}
